{
    "name": "ishannz/secure-elemental-userforms",
    "description": "Adds a secure usersforms",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "element",
        "elemental",
        "content blocks",
        "userforms"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "John Milmine",
            "email": "john.milmine@dna.co.nz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^4.10",
        "dnadesign/silverstripe-elemental": "^4.0",
        "dnadesign/silverstripe-elemental-userforms": "^3.3",
        "madmatt/silverstripe-encrypt-at-rest": "dev-master",
        "silverstripe/userforms": "^5.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "ishannz\\SecureUserForms\\": "src/",
            "ishannz\\SecureUserForms\\Tests\\": "tests/php/"
        }
    }
}
